[DMXPort]
Device=/dev/ttyAMA0
# Only set to non default for testing 
# Speed=250000

[HTTP]
Port=8081
# No authentication if user not set
#User=admin
#Password=password
# PAth to files served by HTTP. No trailing '/'
Root=@localstatedir@/lib/dmx2c2ip/www
# File served if trying to access root URL of server
RootFile=index.xhtml

[C2IP]
# Comma separated list of addresses
Addresses=255.255.255.255

[DB]
File=@localstatedir@/lib/dmx2c2ip/state.db
